pipeline {
	agent {		
		docker { image 'node:18.16.0-alpine' }  
	}

	stages {
	
		stage('Checkout') {
			steps {
				sh '''ls -la'''
				echo 'step Git Checkout'
				sh '''
					pwd
					cat /etc/os-release
					node --version
					python3 --version
					python3 print.py
				'''
				
			}
		}
		
	}
}


